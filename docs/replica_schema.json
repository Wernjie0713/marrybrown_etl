{
  "tables": [
    {
      "name": "APP_4_SALES",
      "module": "Sales & Transactions",
      "purpose": "POS receipt header; one row per sale.",
      "columns": [
        {"name": "ID", "type": "bigint", "notes": "Clustered PK; join helper when surrogate numeric ID is preferred."},
        {"name": "SALES_NO", "type": "varchar", "notes": "Human-readable receipt number; FK target for sales items & payments."},
        {"name": "DATETIME__SALES_DATE", "type": "date", "notes": "Business date used for T-0/T-1 filtering."},
        {"name": "NOW_TIME", "type": "varchar", "notes": "HHMMSS stored as text; needed to reconstruct timestamps."},
        {"name": "SALE_LOCATION", "type": "varchar", "notes": "GUID referencing LOCATION_DETAIL.ID."},
        {"name": "CUSTOMER_ID", "type": "varchar", "notes": "Links to APP_4_CUSTOMER.ID; blank when walk-in."},
        {"name": "ORDER_SOURCE", "type": "varchar", "notes": "Channel indicator (XILNEXKIOSK, XILNEXLIVESALES, etc.)."},
        {"name": "SALES_TYPE", "type": "varchar", "notes": "DINE IN, TAKE AWAY, DELIVERY, RETURN, etc."},
        {"name": "SUBSALES_TYPE", "type": "varchar", "notes": "Secondary channel such as Catering / Birthday."},
        {"name": "SALES_STATUS", "type": "varchar", "notes": "COMPLETED, CANCELLED, PENDING... kept for API-level filtering."},
        {"name": "DOUBLE_TOTAL_AMOUNT", "type": "decimal", "notes": "Grand total paid by customer (tax-inclusive)."},
        {"name": "DOUBLE_TOTAL_TAX_AMOUNT", "type": "decimal", "notes": "Header-level tax (when present)."},
        {"name": "GRANDTOTAL", "type": "decimal", "notes": "Alias sometimes used in API payloads."},
        {"name": "CASHIER", "type": "varchar", "notes": "Staff display name (used in dim_staff)."},
        {"name": "SALES_PERSON", "type": "varchar", "notes": "Alternate staff field."},
        {"name": "SALES_PERSON_USERNAME", "type": "varchar", "notes": "Login/username backing CASHIER."},
        {"name": "STRING_EXTEND_3", "type": "varchar", "notes": "Terminal ID; join to APP_4_CASHIER_DRAWER.SITEID."},
        {"name": "BATCHID", "type": "varchar", "notes": "POS batch/shift marker; keep for troubleshooting."}
      ]
    },
    {
      "name": "APP_4_SALESITEM",
      "module": "Sales & Transactions",
      "purpose": "Line items per receipt.",
      "columns": [
        {"name": "ID", "type": "bigint", "notes": "Primary key referenced by fact tables."},
        {"name": "SALES_NO", "type": "varchar", "notes": "FK to APP_4_SALES.SALES_NO."},
        {"name": "ITEM_CODE", "type": "varchar", "notes": "Numeric SKU; join to APP_4_ITEM.ATLTERNATE_LOOK_UP."},
        {"name": "ITEM_NAME", "type": "varchar", "notes": "Snapshot of product name at sale time."},
        {"name": "CATEGORY", "type": "varchar", "notes": "Category copied from product."},
        {"name": "INT_QUANTITY", "type": "decimal", "notes": "Quantity sold (supports fractional)."},
        {"name": "DOUBLE_PRICE", "type": "decimal", "notes": "Unit price before tax/discount."},
        {"name": "DOUBLE_SUB_TOTAL", "type": "decimal", "notes": "Line total including tax (TotalAmount)."},
        {"name": "DOUBLE_TOTAL_TAX_AMOUNT", "type": "decimal", "notes": "Item + bill-level tax allocation."},
        {"name": "DOUBLE_TOTAL_DISCOUNT_AMOUNT", "type": "decimal", "notes": "Item-specific discount."},
        {"name": "DOUBLE_COST", "type": "decimal", "notes": "Unit cost used for profit."},
        {"name": "PARENTID", "type": "varchar", "notes": "Combo relationship pointer (rarely populated)."},
        {"name": "SALES_TYPE", "type": "varchar", "notes": "Copied from header for convenience."},
        {"name": "SUBSALES_TYPE", "type": "varchar", "notes": "Copied from header."},
        {"name": "VOUCHER_NO", "type": "varchar", "notes": "Applied promotion reference."},
        {"name": "TAXCODE", "type": "varchar", "notes": "MGST/SST flag."},
        {"name": "MODEL", "type": "varchar", "notes": "Menu model / kiosk metadata."}
      ]
    },
    {
      "name": "APP_4_PAYMENT",
      "module": "Sales & Transactions",
      "purpose": "Tender-level data (supports split payments).",
      "columns": [
        {"name": "ID", "type": "bigint", "notes": "Primary key per tender row."},
        {"name": "INVOICE_ID", "type": "varchar", "notes": "Matches APP_4_SALES.SALES_NO."},
        {"name": "METHOD", "type": "varchar", "notes": "cash, card, voucher, ewallet, etc."},
        {"name": "DOUBLE_AMOUNT", "type": "decimal", "notes": "Amount paid by this method."},
        {"name": "PAYMENTDATETIME", "type": "datetime", "notes": "Timestamp of tender capture."},
        {"name": "BUSINESSDATE", "type": "date", "notes": "Business date used in reports."},
        {"name": "STRING_EXTEND_2", "type": "varchar", "notes": "Detailed card/wallet label (CardType)."},
        {"name": "TENDERAMOUNT", "type": "decimal", "notes": "Amount tendered before change."},
        {"name": "CHANGEAMOUNT", "type": "decimal", "notes": "Change returned to customer."},
        {"name": "EODSESSIONID", "type": "varchar", "notes": "Tie to end-of-day reconciliation."},
        {"name": "ISVOID", "type": "bit", "notes": "Marks voided tenders."}
      ]
    },
    {
      "name": "APP_4_ITEM",
      "module": "Products & Inventory",
      "purpose": "Master catalog including sellable + base items.",
      "columns": [
        {"name": "ID", "type": "bigint", "notes": "Primary key."},
        {"name": "ITEM_CODE", "type": "varchar", "notes": "Alphanumeric SKU exposed in UI."},
        {"name": "ATLTERNATE_LOOK_UP", "type": "varchar", "notes": "Numeric SKU used in transactions (critical join)."},
        {"name": "ITEM_NAME", "type": "varchar", "notes": "Product name."},
        {"name": "CATEGORY", "type": "varchar", "notes": "Menu grouping (Beverage, Combo, etc.)."},
        {"name": "ITEM_TYPE", "type": "varchar", "notes": "Product type / class."},
        {"name": "ITEM_BRAND", "type": "varchar", "notes": "Brand or sub-brand when provided."},
        {"name": "DOUBLE_SALE_PRICE", "type": "decimal", "notes": "Current list price."},
        {"name": "BOOL_DISABLED", "type": "tinyint", "notes": "0 = active, 1 = disabled."},
        {"name": "BOOL_ISPACKAGE", "type": "tinyint", "notes": "Flag for combo/package items."},
        {"name": "ADDITIONAL_ITEM_TYPE", "type": "varchar", "notes": "Recipe indicator."}
      ]
    },
    {
      "name": "APP_4_STOCK",
      "module": "Products & Inventory",
      "purpose": "On-hand quantities per item/location.",
      "columns": [
        {"name": "ID", "type": "bigint", "notes": "Primary key."},
        {"name": "ITEM_CODE", "type": "varchar", "notes": "Numeric SKU referencing APP_4_ITEM."},
        {"name": "LOCATION", "type": "varchar", "notes": "GUID referencing LOCATION_DETAIL.ID."},
        {"name": "INT_ONHAND_QUANTITY", "type": "decimal", "notes": "Quantity on hand (weight/volume supported)."}
      ]
    },
    {
      "name": "APP_4_CUSTOMER",
      "module": "Customers & Loyalty",
      "purpose": "CRM master table.",
      "columns": [
        {"name": "ID", "type": "bigint", "notes": "Primary key referenced by APP_4_SALES."},
        {"name": "CUSTOMER_CODE", "type": "varchar", "notes": "Membership / card number."},
        {"name": "FIRST_NAME", "type": "varchar", "notes": "Customer first name (often 'GUEST')."},
        {"name": "LAST_NAME", "type": "varchar", "notes": "Customer last name."},
        {"name": "COMPANY_NAME", "type": "varchar", "notes": "Used when account is corporate."},
        {"name": "MOBILE", "type": "varchar", "notes": "Primary contact; often unique for app users."},
        {"name": "EMAIL", "type": "varchar", "notes": "Optional contact."},
        {"name": "CUSTOMER_GROUP", "type": "varchar", "notes": "Segment/loyalty tier."},
        {"name": "DOUBLE_ACCUMULATE_VALUE", "type": "decimal", "notes": "Current loyalty points (actual field in use)."},
        {"name": "DOUBLE_POINT_VALUE", "type": "decimal", "notes": "Legacy column (observed NULL but keep for parity)."},
        {"name": "BOOL_ACTIVE", "type": "tinyint", "notes": "Account active flag."},
        {"name": "DATETIME__CREATION_DATE", "type": "datetime", "notes": "Registration timestamp."},
        {"name": "DATETIME__DOB", "type": "datetime", "notes": "Date of birth (00-00-0000 treated as NULL)."}
      ]
    },
    {
      "name": "APP_4_POINTRECORD",
      "module": "Customers & Loyalty",
      "purpose": "Loyalty point transaction log.",
      "columns": [
        {"name": "ID", "type": "bigint", "notes": "Primary key per point transaction."},
        {"name": "CLIENT_ID", "type": "varchar", "notes": "FK to APP_4_CUSTOMER.ID."},
        {"name": "SALESID", "type": "varchar", "notes": "FK to APP_4_SALES.SALES_NO."},
        {"name": "DOUBLE_ACCUMULATE_VALUE", "type": "decimal", "notes": "Points earned/spent in this transaction."},
        {"name": "DOUBLE_POINT_VALUE", "type": "decimal", "notes": "Legacy points column (NULL but retained)."},
        {"name": "REMARK", "type": "varchar", "notes": "Descriptive text (e.g., Sales ID)."},
        {"name": "DATETIME__RECORD_DATE", "type": "date", "notes": "Point event date."}
      ]
    },
    {
      "name": "LOCATION_DETAIL",
      "module": "Outlets",
      "purpose": "Authoritative outlet master.",
      "columns": [
        {"name": "LOCATION_INT_ID", "type": "int", "notes": "Clustered PK."},
        {"name": "ID", "type": "varchar", "notes": "GUID referenced by sales, stock, etc."},
        {"name": "LOCATIONNAME", "type": "varchar", "notes": "Display name used in reports."},
        {"name": "LOCSTATUS", "type": "varchar", "notes": "Operational status (Active/Closed)."},
        {"name": "LOCATION_DELETED", "type": "tinyint", "notes": "Soft-delete flag (0=active)."},
        {"name": "ADDRESS1", "type": "varchar", "notes": "Street info if available."},
        {"name": "CITY", "type": "varchar", "notes": "Parsed city (if stored)."},
        {"name": "STATE", "type": "varchar", "notes": "State/region."},
        {"name": "POSTCODE", "type": "varchar", "notes": "Postal code."}
      ]
    },
    {
      "name": "APP_4_VOUCHER_MASTER",
      "module": "Promotions",
      "purpose": "Promotion / voucher definitions.",
