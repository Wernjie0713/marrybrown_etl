        {"name": "ID", "type": "bigint", "notes": "Primary key; referenced by voucher_no."},
        {"name": "RULE_TITLE", "type": "varchar", "notes": "Promotion name."},
        {"name": "RULE_DESCRIPTION", "type": "varchar", "notes": "Long description."},
        {"name": "REWARD_CODE", "type": "varchar", "notes": "Voucher/promo code used at POS."},
        {"name": "VOUCHER_MASTER_TYPE", "type": "int", "notes": "1=Product Deal, 2=Discount, 4=Gift, etc."},
        {"name": "DATETIME__ACTIVATE_DATE", "type": "datetime", "notes": "Start date."},
        {"name": "DATETIME__EXPIRY_DATE", "type": "datetime", "notes": "End date."},
        {"name": "IS_ACTIVATE", "type": "tinyint", "notes": "Activation flag."},
        {"name": "WBDELETED", "type": "tinyint", "notes": "Soft delete indicator."}
      ]
    },
    {
      "name": "APP_4_CASHIER_DRAWER",
      "module": "Sales & Transactions",
