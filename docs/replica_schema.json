        {"name": "SITEID", "type": "varchar", "notes": "Terminal identifier referenced via STRING_EXTEND_3."},
        {"name": "BRANCHID", "type": "varchar", "notes": "GUID referencing LOCATION_DETAIL.ID."},
        {"name": "TERMINALNAME", "type": "varchar", "notes": "Friendly name (may need to be derived)."}
      ]
    },
    {
      "name": "APP_4_VOIDSALESITEM",
